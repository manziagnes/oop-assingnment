class Vehicle:
    def move(self):
        raise NotImplementedError("Subclasses must implement this method")

class Car(Vehicle):
    def move(self):
        return "Driving ğŸš—"

class Plane(Vehicle):
    def move(self):
        return "Flying âœˆï¸"

class Boat(Vehicle):
    def move(self):
        return "Sailing ğŸš¤"

# Creating instances
car = Car()
plane = Plane()
boat = Boat()

# Using polymorphism
vehicles = [car, plane, boat]

for vehicle in vehicles:
    print(vehicle.move())
